<h1> Movie #<%= @movie.id %></h1>
  <dt> Title</dt>
  <dd><%= @movie.title %></dd>

  <dt>Year</dt>
  <dd><%= @movie.year %></dd>

  <dt>Duration</dt>
  <dd><%= @movie.duration %></dd>

  <dt>Description</dt>
  <dd><%= @movie.description %></dd>

  <dt>Image</dt>
  <dd><img src="<%= @movie.image_url %>" width="320"></dd>

  <dt>Destroy?</dt>
  <dd><a href="http://localhost:3000/delete_movie/<%= @movie.id %>">Delete</a></dd>

  <dt>Go Back?</dt>
  <dd><a href ="http://localhost:3000/movies">Home</a></dd>
